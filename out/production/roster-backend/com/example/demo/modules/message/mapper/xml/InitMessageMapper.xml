<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.modules.message.mapper.InitMessageMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.modules.message.entity.InitMessage">
            <id property="id" column="id" />
            <result property="content" column="content" />
            <result property="title" column="title" />
            <result property="message_type" column="message_type" />
            <result property="parent_id" column="parent_id" />
            <result property="create_time" column="create_time" />
            <result property="status" column="status" />
            <result property="url" column="url" />
    </resultMap>

    <sql id="Base_Column_List">
        id,content,title,message_type,parent_id,create_time,
        status,url
    </sql>
</mapper>
